@{
    ViewData["Title"] = "Home Page";
}




<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-3 col-md-6">
            <div class="panel panel-dashboard" id="hell">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-12 text-center">
                            <div class="controls left"><i class="ion ion-more"></i></div>
                            <div class="controls right"><a class="remove"><i class="ion ion-minus-round"></i></a></div>
                            <div class="center">Helligkeit</div>
                        </div>
                    </div>
                </div>
                <div class="panel-body">

                    <div class="huge" id="hellWert">100</div>
                    <button class="btn btn-default btn-block" id="hellConnect">Connect</button>
                    <div class="clearfix hell"></div>
                   
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="panel panel-dashboard">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-12 text-center">
                            <div class="controls left"><i class="ion ion-more"></i></div>
                            <div class="controls right"><a class="remove"><i class="ion ion-minus-round"></i></a></div>
                            <div class="center">Zeit Intervall </div>
                        </div>
                    </div>
                </div>
                <div class="panel-body">
                    <div class="huge" id="hellZeitWert">5</div>
                    <button class="btn btn-default btn-block" id="hellÄndern">Ändern</button>
                    <div class="clearfix" id="hellDiv"></div>
                    <div id="newDiv1"><input type="text" name="hellZeitWert" id="inputHell" placeholder="neuer Wert"><br><button class="btn btn-default btn-block" id="sendBtnHell">Send</button></div>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="panel panel-dashboard" id="temp">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-12 text-center">
                            <div class="controls left"><i class="ion ion-more"></i></div>
                            <div class="controls right"><a class="remove"><i class="ion ion-minus-round"></i></a></div>
                            <div class="center">Temperatur</div>
                        </div>
                    </div>
                </div>
                <div class="panel-body">
                    <div class="huge" id="tempWert">20</div>
                    <button class="btn btn-default btn-block" id="tempConnect">Connect</button>
                    <div class="clearfix temp"></div>
                </div>

            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="panel panel-dashboard">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-12 text-center">
                            <div class="controls left"><i class="ion ion-more"></i></div>
                            <div class="controls right"><a class="remove"><i class="ion ion-minus-round"></i></a></div>
                            <div class="center">Zeit Intervall </div>
                        </div>
                    </div>
                </div>
                <div class="panel-body">

                    <div class="huge" id="tempZeitWert">20</div>
                    <button class="btn btn-default btn-block" id="tempÄndern">Ändern</button>
                    <div class="clearfix"></div>
                    <div id="newDiv2"><input type="text" name="tempZeitWert" id="inputTemp" placeholder="neuer Wert"><br><button class="btn btn-default btn-block" id="sendBtnTemp">Send</button></div>
                </div>

            </div>
        </div>
    </div>
    <!-- /.row -->
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-dashboard">
                <div class="panel-heading">
